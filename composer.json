{
	"name": "squadron-api/crud",
	"description": "Squadron API CRUD helpers",
	"keywords": ["laravel", "squadron", "crud", "helpers", "api"],
	"license": "MIT",
	"authors": [{
		"name": "Andrey Novikov",
		"email": "andrew@7glyphs.com",
		"homepage": "https://7glyphs.com",
		"role": "Developer"
	}],
	"require": {
		"php": ">=7.2",
		"illuminate/database": "^5.8",
		"illuminate/support": "^5.8",
		"illuminate/pagination": "^5.8",
        "squadron-api/base": "0.0.*"
	},
	"require-dev": {
        "orchestra/testbench": "~3",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Squadron\\CRUD\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Squadron\\CRUD\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Squadron\\CRUD\\ServiceProvider"
            ]
        }
    }
}
